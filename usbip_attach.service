[Unit]
Description=USB-IP: Binding all devices from binding file at ~/.config/usbip (Host %I)
After=network-online.target

[Service]
Type=simple
ExecStart=/usr/bin/python %h/.config/usbip/usbip-scripts/service_script.py --attach --device-list=%h/.config/usbip/devices --host=%I

[Install]
WantedBy=default.target
